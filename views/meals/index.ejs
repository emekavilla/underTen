  <!-- <h1>Under$10 List</h1> -->
  <% meals.forEach(function(meals) { %>
   <body>
     <div class="row">
        <div class="col s12 m6">
          <div class="card #3949ab indigo darken-1">
            <div class="card-content h6 white-text">
              <span class="card-title"  '<%= `/meals/${meals.id}` %>'><span><%= meals.amount %></span>
              <ul>
                  <li '<%= `/meals/${meals.id}` %>'><span><%= meals.meal_name %></span></li>
                  <li '<%= `/meals/${meals.id}` %>'><span><%= meals.restaurant_name %></span></li>
                  <li '<%= `/meals/${meals.id}` %>'><span><%= meals.location %></span></li>
                  <li '<%= `/meals/${meals.id}` %>'><span><%= meals.review %></span></li>
                  <li '<%= `/meals/${meals.id}` %>'><span><%= meals.createdAt %></span></li>
                  </ul>
            </div>
            <div class="card-action">
              <a href="#">Add meal</a>
              <a href="#">Delete meal</a>


              <form action="<%= '/meals/' + meals.id + '?_method=DELETE' %>" method="POST">
                <input type="submit" value="Delete" />
                  </form>
                  <form action="<%='/meals/' + meals.id + '/edit' %>" method="POST">
                <input type="submit" value="Edit" />
              </form>
            </div>
            <!-- <div class="card-action">
              <a href="<%='/meals/' + meals.id + '/edit' %>" method="POST">Add meal</a>
              <a href="<%= '/meals/' + meals.id + '?_method=DELETE' %>" method="POST" >Delete meal</a>
            </div>
          </div>
        </div> -->
</body>
   <% }) %>
