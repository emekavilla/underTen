<body>
  <nav>
    <div class="nav-wrapper #ffd54f amber lighten-2">
      <a href="#" class="brand-logo center ">Welcome to Under$10</a>
    </div>
  </nav>
  <!-- <h1>Under$10 List</h1> -->
  <% meals.forEach(function(meals) { %>
  <div class="row">
    <div class="col s12 m6">
      <div class="card #3949ab indigo darken-1">
        <div class="card-content h6 white-text">
          <a class="card-title"  href="<%= `/meals/${meals.id}` %>"><%= meals.amount %></a>
          <ul>
            <li><%= meals.meal_name %></li>
            <li><%= meals.restaurant_name %></li>
            <li><%= meals.location %></li>
            <li><%= meals.review %></li>
            <li><%= meals.createdAt %></li>
          </ul>
        </div>
        <div class="card-action">
          <a href="#">Add meal</a>
          <a href="#">Delete meal</a>
          <form action="<%= '/meals/' + meals.id + '?_method=DELETE' %>" method="POST">
            <input type="submit" value="Delete" />
          </form>
          <form action="<%='/meals/' + meals.id + '/edit' %>" method="POST">
            <input type="submit" value="Edit" />
          </form>
        </div>
      </div>
    </div>
  </div>
  <% }) %>

  <a class="waves-effect waves-light btn-large #3949ab indigo darken-1 ">Log Out</a>
  <footer class="page-footer #ffd54f amber lighten-2">
    <div class="container #ffd54f amber lighten-2">
      <div class="row">
        <div class="col l6 s12">
          <h5 class="#3949ab indigo-text darken-1-text">Under$10</h5>
          <p class="#3949ab indigo-text darken-1-text">General Assembly Group Project Unit 3</p>
        </div>
        <div class="col l4 offset-l2 s12">
          <h5 class="#3949ab indigo-text darken-1-text">Happy Eating!</h5>
        </div>
      </div>
    </div>
    <div class="footer-copyright">
      <div class="container">
        © 2017 Copyright
        <a class="#3949ab indigo-text darken-1 right" href="#!">nom nom nom</a>
      </div>
    </div>
  </footer>
  <!--Import jQuery before materialize.js-->
  <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.3/js/materialize.min.js"></script>
</body>
   <% }) %>
